
program Main
    write(1)         // 'write' no longer prints a new line character afterwards
    writeNewLine()   // 'writeNewLine' prints just a new line character

    writeln(2)       // 'writeln' is write with a new line afterwards

    writeChar(97)    // 'writeChar' is writing with conversion to a char in Python
    writeNewLine()
    writeCharLn(97)  // 'writeCharLn' is writing with conversion to a char in Python followed by a new line afterwards
